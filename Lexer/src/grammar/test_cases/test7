int :: a, b, c;

if(a < b)
    if(a == 0)
        b = 100;
    end if;
elif (a > b)
    if (b == 0)
        a = 100;
    end if;
else
    a = 100;
    b = 100;
end if;

for (c = 100; c > 10; c = c-1)
    if(a < b)
        if(a == 0)
            b = 100;
        end if;
    elif (a > b)
        if (b == 0)
            a = 100;
        end if;
    else
        a = 100;
        b = 100;
    end if;
end for;